{
  "version": 2,
  "buildCommand": "npm run build:packages && cd client && npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/[...slug].js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
